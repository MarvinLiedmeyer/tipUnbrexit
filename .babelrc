{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 3 versions"
          ]
        },
        "modules": false
      }
    ],
    "stage-0",
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties",
    "react-hot-loader/babel",
    ["transform-imports", {
      "chayns-components": {
        "transform": "../chayns-components/lib/utils/babel/resolveAbsoluteImport.js",
        "preventFullImport": true
      }
    }]
  ]
}